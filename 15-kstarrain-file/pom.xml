        <!-- 处理IO的工具类包，对java.io进行扩展，提供了更加方便的IO操作 -->
        <dependency>
            <groupId>commons-io</groupId>
            <artifactId>commons-io</artifactId>
            <version>2.6</version>
        </dependency>

        <!-- common 工具类-->
        <dependency>
            <groupId>com.kstarrain</groupId>
            <artifactId>0-kstarrain-common</artifactId>
